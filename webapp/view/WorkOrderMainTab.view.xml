<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns:table="sap.ui.table" controllerName="ZWMReplacement.controller.WorkOrderMainTab" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page >
		<customHeader>
			<Bar>
				<contentLeft>
					<Text text="Sap Work Manager"/>
				</contentLeft>
			</Bar>
		</customHeader>
		<content>
			<IconTabBar id="idIconTabBarNoIcons" expanded="{device>/isNoPhone}" class="sapUiResponsiveContentPadding">
				<items>
					<IconTabFilter text="Work Orders" press="onMasterDetail">
						<SplitApp id="idWOMainSplit" initialDetail="detail" initialMaster="master" orientationChange="onOrientationChange"></SplitApp>
					</IconTabFilter>
					<IconTabFilter text="Notifications"></IconTabFilter>
					<IconTabFilter text="Timesheets"></IconTabFilter>
					<IconTabFilter text="Functional Location"></IconTabFilter>
					<IconTabFilter text="Equipment"></IconTabFilter>
				</items>
			</IconTabBar>
		</content>
		<footer>
			<Toolbar>
				<ToolbarSpacer/>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>