<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form"
	controllerName="ZWMReplacement.controller.WM_Detail" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page>
		<customHeader>
			<Bar>
				<contentLeft>
					<Text text="Work Order : {/?orderData/Aufnr} - Received "/>
				</contentLeft>
				<contentRight>
					<Button icon="sap-icon://edit" press="onPressEdit" ariaLabelledBy="editButtonLabel"/>
					<Button icon="sap-icon://action" press="onClick" ariaLabelledBy="actionButtonLabel"/>
				</contentRight>
			</Bar>
		</customHeader>
		<IconTabBar id="idIconTabBarNoIcons" expanded="{device>/isNoPhone}" class="sapUiResponsiveContentPadding">
			<items>
				<IconTabFilter text="Detail">
					<content>
						<mvc:XMLView viewName="ZWMReplacement.view.DetailTab"/>
					</content>
				</IconTabFilter>
				<IconTabFilter text="Notification">
					<content>
						<mvc:XMLView viewName="ZWMReplacement.view.Notification_Tab"/>
					</content>
				</IconTabFilter>
				<IconTabFilter text="Operations">
						<l:VerticalLayout class="sapUiContentPadding" width="100%">
						<l:content>
							<List id="notificationList1">
								<NotificationListItem title="1000000033: Breather-NA " description="20" showCloseButton="false" authorName="Required 1 EA "
									datetime="Withdrawn 0 EA" unread="true" priority="None" press="onListItemPressOperations"/>
							</List>
						</l:content>
					</l:VerticalLayout>
				</IconTabFilter>
				<IconTabFilter text="Components">
					<l:VerticalLayout class="sapUiContentPadding" width="100%">
						<l:content>
							<List id="notificationList">
								<NotificationListItem title="1000000032: Breather-NA " description="20" showCloseButton="false" authorName="Required 1 EA "
									datetime="Withdrawn 0 EA" unread="true" priority="None" press="onListItemPress"/>
							</List>
						</l:content>
					</l:VerticalLayout>
				</IconTabFilter>
				<IconTabFilter text="Objects"></IconTabFilter>
				<IconTabFilter text="Attachments"></IconTabFilter>
				<IconTabFilter text="Maps"></IconTabFilter>
			</items>
		</IconTabBar>
	</Page>
</mvc:View>