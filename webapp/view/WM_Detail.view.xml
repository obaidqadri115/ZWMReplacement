<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form"
	controllerName="ZWMReplacement.controller.WM_Detail" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page>
		<customHeader>
			<Bar>
				<contentLeft>
					<Text text="Work Order : {/?orderData/Aufnr} - Received "/>
				</contentLeft>
				<contentRight>
					<Button icon="sap-icon://edit" press="onPress" ariaLabelledBy="editButtonLabel"/>
					<Button icon="sap-icon://action" press="onClick" ariaLabelledBy="actionButtonLabel"/>
				</contentRight>
			</Bar>
		</customHeader>
		<IconTabBar id="idIconTabBarNoIcons" expanded="{device>/isNoPhone}" class="sapUiResponsiveContentPadding">
			<items>
				<IconTabFilter text="Detail">
					<mvc:XMLView viewName="ZWMReplacement.view.DetailTab"/>
				</IconTabFilter>
				<IconTabFilter text="Notification"></IconTabFilter>
				<IconTabFilter text="Operations"></IconTabFilter>
				<IconTabFilter text="Components"></IconTabFilter>
				<IconTabFilter text="Objects"></IconTabFilter>
				<IconTabFilter text="Attachments"></IconTabFilter>
				<IconTabFilter text="Maps"></IconTabFilter>
			</items>
		</IconTabBar>
	</Page>
</mvc:View>