{
	"_version": "1.7.0",
	"sap.app": {
		"id": "ZWMReplacement",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZEAM_WM_FIORI_APP_SRV": {
				"uri": "/sap/opu/odata/sap/ZEAM_WM_FIORI_APP_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "ZWMReplacement.view.Main",
			"type": "XML"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "ZWMReplacement.view",
				"controlId": "app",
				"viewType": "XML",
				"controlAggregation": "pages",

				"async": true
			},
			"routes": [{
					"name": "workOrderMainTab",
					"pattern": "",
					"target": "workOrderMainTab"

				}, {
					"name": "master",
					"pattern": "dummy",
					"target": [
						"master",
						"orderDetails"
					]
				}, {
					"name": "orderDetails",
					"pattern": "orders/:?orderData:",
					"target": [
						"master",
						"orderDetails"
					]

				}, {
					"name": "componentMasterDetail",
					"pattern": "componentMasterDetail",
					"target": "componentMasterDetail"

				},

				{
					"name": "operationMasterDetail",
					"pattern": "operationMasterDetail",
					"target": "operationMasterDetail"

				}, {
					"name": "editWO",
					"pattern": "editWO",
					"target": "editWO"

				}
			],
			"targets": {
				"workOrderMainTab": {
					"viewName": "WorkOrderMainTab"
				},

				"master": {
					"viewName": "WM_Master",
					"parent": "splitviewTarget",
					"controlId": "splitContainerControl",
					"controlAggregation": "masterPages",
					"viewLevel": 0
				},
				"orderDetails": {
					"viewName": "WM_Detail",
					"parent": "splitviewTarget",
					"controlId": "splitContainerControl",
					"controlAggregation": "detailPages",
					"viewLevel": 1
				},

				"componentMasterDetail": {
					"viewName": "ComponentMasterDetail"
				},
				"operationMasterDetail": {
					"viewName": "OperationMasterDetail"
				},
				"editWO": {
					"viewName": "EditWorkOrder"
				}

			}
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ZWMReplacement.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}